<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage 
    xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    x:Class="MyDietSchedule.Views.Users.UserManagementView"
    xmlns:local="clr-namespace:MyDietSchedule.Views.Users">

    <NavigationPage 
        Title="User Information"
        x:Name="NewUser"
        Icon="newUser.png"
        BarBackgroundColor="{StaticResource ThirdColor}"
        BarTextColor="{StaticResource SecondayColor}">
        <x:Arguments>
            <local:NewUser/>
        </x:Arguments>
    </NavigationPage>
</TabbedPage>
